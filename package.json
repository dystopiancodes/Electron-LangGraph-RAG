{
  "name": "electron-langgraph-rag",
  "version": "1.0.0",
  "description": "Electron app with LangGraph adaptive RAG",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "@langchain/community": "^0.0.26",
    "@langchain/core": "^0.1.26",
    "@langchain/langgraph": "^0.0.7",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "faiss-node": "^0.5.1",
    "langchain": "^0.1.9",
    "node-fetch": "^2.6.1",
    "pdf-parse": "^1.1.1",
    "web-streams-polyfill": "^3.2.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "extraResources": [
      {
        "from": "node_modules/pdf-parse",
        "to": "pdf-parse"
      },
      {
        "from": "node_modules/node-ensure",
        "to": "node-ensure"
      },
      {
        "from": "node_modules/faiss-node",
        "to": "faiss-node"
      }
    ],
    "asar": true,
    "asarUnpack": [
      "node_modules/faiss-node/**/*"
    ]
  }
}
